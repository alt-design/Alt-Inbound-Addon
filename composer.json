{
    "name": "alt-design/alt-inbound",
    "description": "Alt Inbound addon, adds IP blocking to your site",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AltDesign\\AltInbound\\": "src"
        }
    },
    "require": {
        "php": "^8.1",
        "statamic/cms": "^4.0|^5.0"
    },
    "extra": {
        "statamic": {
            "name": "Alt Inbound",
            "description": "Alt Inbound addon"
        },
        "laravel": {
            "providers": [
                "AltDesign\\AltInbound\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": false
        }
    }
}
